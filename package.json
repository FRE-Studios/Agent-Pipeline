{
  "name": "agent-pipeline",
  "version": "0.1.0",
  "description": "Intelligent agent orchestration with parallel execution, conditional logic, git workflow automation, and multi-channel notifications for Claude Code",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "agent-pipeline": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc && node scripts/copy-templates.mjs",
    "dev": "tsc --watch",
    "test": "vitest",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "dist/**/*.js.map",
    "dist/**/*.yml",
    "dist/**/*.md",
    "!dist/**/*.test.*",
    "!dist/__tests__",
    "README.md",
    "license",
    "CLAUDE.md"
  ],
  "keywords": [
    "agent",
    "pipeline",
    "claude",
    "ai",
    "automation",
    "git",
    "ci-cd",
    "workflow",
    "orchestration",
    "parallel-execution",
    "dag",
    "notifications"
  ],
  "author": "Fernando Rojas <fernando@fre-studios.com> (https://fre-studios.com/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FRE-Studios/agent-pipeline.git"
  },
  "homepage": "https://github.com/FRE-Studios/agent-pipeline#readme",
  "bugs": {
    "url": "https://github.com/FRE-Studios/agent-pipeline/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/FRE-Studios"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.0",
    "@types/node-notifier": "^8.0.5",
    "ink": "^4.4.1",
    "ink-spinner": "^5.0.0",
    "node-notifier": "^10.0.1",
    "react": "^18.2.0",
    "simple-git": "^3.25.0",
    "tiktoken": "^1.0.22",
    "uuid": "^10.0.0",
    "yaml": "^2.4.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/react": "^18.2.0",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "ink-testing-library": "^4.0.0",
    "typescript": "^5.5.0",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
